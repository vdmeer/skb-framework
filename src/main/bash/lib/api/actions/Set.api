FW_API["Set%object%phase%@ID@%print-level%to%(%all%|%none%|%@LEVEL@%)"]="sets print level for phase ID to all, none, or LEVEL"
FW_API["Set%object%phase%@ID@%log-level%to%(%all%|%none%|%@LEVEL@%)"]="sets log level for phase ID to all, none, or LEVEL"

FW_API["Set%object%settings%@ID@%to%@VALUE@"]="sets setting ID to VALUE"

FW_API["Set%object%themeitem%@ID@%to%@VALUE@"]="sets theme item ID to VALUE"

FW_API["Set%element%application%@ID@%command%to%@COMMAND@"]="sets applcation ID command to COMMAND"
    FW_API["Set%element%application%@ID@%to%@COMMAND@%@ARGNUM@%@ARGS@"]="sets applcation ID properties: COMMAND, ARGNUM, ARGS"

FW_API["Set%app%name%to%@VALUE@"]="sets application name to VALUE"
FW_API["Set%app%name2%to%@VALUE@"]="sets application name 2 to VALUE"

FW_API["Set%config%file%to%@FILE@"]="sets config file to FILE"

FW_API["Set%current%mode%to%@MODE@"]="sets current mode to MODE"
FW_API["Set%current%phase%to%@PHASE@"]="sets current phase to PHASE and copies errors/warnings"
FW_API["Set%current%theme%to%@THEME@"]="sets current theme to THEME"

FW_API["Set%current%project%to%@PROJECT@"]="sets current project to PROJECT"
FW_API["Set%last%project%to%@PROJECT@"]="sets last project to PROJECT"

FW_API["Set%current%scenario%to%@SCENARIO@"]="sets current scenario to SCENARIO"
FW_API["Set%last%scenario%to%@SCENARIO@"]="sets last scenario to SCENARIO"

FW_API["Set%current%site%to%@SITE@"]="sets current site to SITE"
FW_API["Set%last%site%to%@SITE@"]="sets last site to SITE"

FW_API["Set%current%task%to%@TASK@"]="sets current task to TASK"
FW_API["Set%last%task%to%@TASK@"]="sets last task to TASK"

FW_API["Set%log%format%to%@FORMAT@"]="sets log format to FORMAT"
FW_API["Set%print%format%to%@FORMAT@"]="sets print format to FORMAT"
FW_API["Set%print%format2%to%@FORMAT@"]="sets print format 2 to FORMAT"

FW_API["Set%print%level%to%(%all%|%none%|%@LEVEL@%)"]="sets print level to all, none, or LEVEL"
FW_API["Set%log%level%to%(%all%|%none%|%@LEVEL@%)"]="sets log level to all, none, or LEVEL"

FW_API["Set%log%date-arg%to%@VALUE@"]="sets log date-arg to VALUE"
FW_API["Set%log%dir%to%@DIR@"]="sets log dir to DIR"
FW_API["Set%log%file%to%@FILE@"]="sets log file to FILE"

FW_API["Set%module%path%to%@PATH@"]="sets module path to PATH"
